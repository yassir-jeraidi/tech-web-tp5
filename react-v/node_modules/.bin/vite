#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/dev/enset/tech-web/tp5/react-v/node_modules/.pnpm/vite@6.2.2_@types+node@22.13.10_jiti@2.4.2_lightningcss@1.29.2/node_modules/vite/bin/node_modules:/Volumes/dev/enset/tech-web/tp5/react-v/node_modules/.pnpm/vite@6.2.2_@types+node@22.13.10_jiti@2.4.2_lightningcss@1.29.2/node_modules/vite/node_modules:/Volumes/dev/enset/tech-web/tp5/react-v/node_modules/.pnpm/vite@6.2.2_@types+node@22.13.10_jiti@2.4.2_lightningcss@1.29.2/node_modules:/Volumes/dev/enset/tech-web/tp5/react-v/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Volumes/dev/enset/tech-web/tp5/react-v/node_modules/.pnpm/vite@6.2.2_@types+node@22.13.10_jiti@2.4.2_lightningcss@1.29.2/node_modules/vite/bin/node_modules:/Volumes/dev/enset/tech-web/tp5/react-v/node_modules/.pnpm/vite@6.2.2_@types+node@22.13.10_jiti@2.4.2_lightningcss@1.29.2/node_modules/vite/node_modules:/Volumes/dev/enset/tech-web/tp5/react-v/node_modules/.pnpm/vite@6.2.2_@types+node@22.13.10_jiti@2.4.2_lightningcss@1.29.2/node_modules:/Volumes/dev/enset/tech-web/tp5/react-v/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/vite@6.2.2_@types+node@22.13.10_jiti@2.4.2_lightningcss@1.29.2/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../.pnpm/vite@6.2.2_@types+node@22.13.10_jiti@2.4.2_lightningcss@1.29.2/node_modules/vite/bin/vite.js" "$@"
fi
